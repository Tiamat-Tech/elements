{
  "name": "@cndycd/core",
  "version": "0.0.0",
  "description": "",
  "homepage": "https://github.com/cndycd/core",
  "bugs": "https://github.com/cndycd/core/issues",
  "license": "UNLICENSED",
  "author": "Sophia Michelle Andren <sophia@candycode.co> (http://candycode.co/)",
  "files": [
    "**"
  ],
  "module": "index.js",
  "main": "index.cjs.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs.js",
      "node": "./index.cjs.js",
      "types": "./index.d.ts"
    },
    "./carousel": {
      "import": "./carousel.js",
      "require": "./carousel.cjs.js",
      "node": "./carousel.cjs.js",
      "types": "./carousel.d.ts"
    },
    "./disclosure": {
      "import": "./disclosure.js",
      "require": "./disclosure.cjs.js",
      "node": "./disclosure.cjs.js",
      "types": "./disclosure.d.ts"
    }
  },
  "sideEffects": false,
  "repository": "github:cndycd/core",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "rollup -c",
    "postbuild": "yarn copy",
    "eslint": "eslint --fix '{src}/**/*.{js,ts,jsx,tsx}'",
    "eslint:ci": "eslint '{src}/**/*.{js,ts,jsx,tsx}'",
    "prepare": "yarn build",
    "pretest": "tsc --noEmit",
    "copy": "shx cp dist/index.d.ts dist/index.cjs.d.ts && shx cp dist/carousel.d.ts dist/carousel.cjs.d.ts && shx cp dist/disclosure.d.ts dist/disclosure.cjs.d.ts && copyfiles -f package.json README.md LICENSE dist && json -I -f dist/package.json -e \"this.scripts=undefined, this.devDependencies=undefined, this.private=false; this.husky = undefined, this['lint-staged']=undefined\""
  },
  "peerDependencies": {
    "classnames": "^2.2.0",
    "jotai": "^0.12.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-intersection-observer": "^8.30.0",
    "react-spring": "^9.0.0-rc.0",
    "react-use-gesture": "^9.0.0-beta.0",
    "react-use-measure": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "classnames": "^2.2.6",
    "copyfiles": "^2.4.1",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=4",
    "jotai": "^0.12.2",
    "json": "^10.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-scripts": "^4.0.1",
    "react-spring": "^9.0.0-rc.3",
    "react-use-gesture": "^9.0.0-beta.11",
    "react-use-measure": "^2.0.3",
    "rollup": "^2.36.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "shx": "^0.3.3",
    "typescript": "^4.1.3"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,json}": "prettier --write"
  }
}
